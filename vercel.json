{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "vite",
    "functions": {
        "api/**/*.ts": {
            "runtime": "@vercel/node@3.0.0"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ]
}